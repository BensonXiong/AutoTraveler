<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!--Appium 服务关键字-->
    <capability>
        <appiumVersion>1.0</appiumVersion>
        <autoLaunch>true</autoLaunch>
        <noReset>false</noReset>
        <language>zh-Hans</language>
    </capability>
    <iosCapability>
        <app>/Users/mac/Desktop/apk/PANewToapAPP.app</app>
        <udid>d32975a39224bc409796bcdc75956cd6530edbdb</udid>
        <bundleId>com.pingan.yizhangtong</bundleId>
        <platformName>iOS</platformName>
        <platformVersion>8.2</platformVersion>
        <deviceName>iPhone 5</deviceName>
        <autoAcceptAlerts>true</autoAcceptAlerts>
    </iosCapability>
    <!--引导流 关键字驱动,引导至所需模块再开始遍历-->
    <iosGuideFlow>
        <!--滑动类型设置-->
        <step>slide>>2</step>
        <!--点击类型设置-->
        <step>click>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIAButton[1]</step>
        <step>click>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAButton[3]</step>
        <!--输入类型设置-->
        <!--<step>input>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIATextField[1]|15618971003</step>-->
        <!--<step>click>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIAButton[1]</step>-->
        <!--<step>input>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIASecureTextField[1]|qwer2015</step>-->
        <!--<step>click>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIAButton[1]</step>-->
        <step>click>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIAButton[2]</step>
        <step>input>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIATextField[1]|15618971003</step>
        <step>input>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIASecureTextField[1]|qwer2015</step>
        <step>click>>xpath:://UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIAButton[1]</step>
        <!--<step>input>>xpath:://UIAApplication[1]/UIAWindow[3]/UIATextField[1]|8888</step>-->
        <!--<step>click>>xpath:://UIAApplication[1]/UIAWindow[3]/UIAButton[2]</step>-->
        <!--手势密码设置-->
        <step>gesture>>xpath:://UIAButton[@name='blue circle']</step>
    </iosGuideFlow>
    <rule>
        <!--窗口鉴定策略,默认取前8个节点生成md5-->
        <identify-default>5</identify-default>
        <!--Tab窗口用selected区别,可能要多选几个节点到达-->
        <identify-special>
            <define>专题,直播,要闻,选股>>16</define>
            <define>简单理财,投资,保险,贷款,信用卡>>13</define>
        </identify-special>
        <!--窗口执行顺序 1:从上到下(默认) 2:从下到上-->
        <reverse>1</reverse>
        <!--控件白名单-->
        <click>
            <class>UIAImage</class>
            <class>UIAButton</class>
            <class>UIASwitch</class>
            <class>UIATableCell</class>
            <!--<class>UIAStaticText</class>-->
            <class>UIAPickerWheel</class>
            <class>UIACollectionCell</class>
        </click>
        <input>
            <class>UIATextField</class>
            <class>UIASearchBar</class>
            <class>UIASecureTextField</class>
        </input>
        <!--返回键-->
        <back>
            <location>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</location>
            <location>//UIAApplication[1]/UIAWindow[1]/UIANavigationBar[1]/UIAButton[2]</location>
            <location>//UIAApplication[1]/UIAWindow[3]/UIAButton[1]</location>
            <location>//UIAApplication[1]/UIAWindow[4]/UIAButton[2]</location>
            <location>//UIAApplication[1]/UIAWindow[2]/UIAToolbar[1]/UIAButton[3]</location>
            <location>//UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIAWebView[1]/UIAImage[1]</location>
            <location>//UIAApplication[1]/UIAWindow[1]/UIAButton[2]</location>
            <location>//UIAApplication[1]/UIAWindow[1]/UIAButton[1]</location>
            <location>//UIAApplication[1]/UIAWindow[3]/UIAButton[1]</location>
            <location>//UIAApplication[1]/UIAWindow[3]/UIAButton[2]</location>
        </back>
        <!--无需返回的列表,需配置识别窗口唯一性的内容-->
        <notback>
            <item>首页,金融旗舰店,财经快讯,我,我的资产,财富加速,财富排名,健康,房产</item>
            <item>首页,金融旗舰店,财经快讯,我,我的订单,活期,定期</item>
            <item>首页,金融旗舰店,财经快讯,我,直播,要闻,专题,选股</item>
            <item>首页,金融旗舰店,财经快讯,我,账户余额,银行卡,订单中心,我的信用</item>
        </notback>
        <!--黑名单 支持text,name,resourceId-->
        <blackList>
            <item>相机</item>
            <item>相册</item>
            <item>照片</item>
            <item>shift</item>
            <item>Dictate</item>
            <item>DONE</item>
            <item>键盘</item>
            <item>平安行</item>
            <item>天天赢</item>
            <item>交易密码</item>
            <item>退出登录</item>
            <item>拍摄名片</item>
            <item>credit card camera</item>
            <item>添加平安信用卡</item>
            <item>如您已完成添加，请重新登录</item>
            <item>重新登录</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIATableView[1]/UIATableCell[1]/UIAButton[1]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIATableView[1]/UIATableCell[1]/UIAButton[1]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIATableView[1]/UIATableCell[1]/UIAButton[2]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIAScrollView[1]/UIATableView[1]/UIATableCell[1]/UIAButton[3]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIATableView[1]/UIATableCell[1]/UIAButton[1]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIATableView[1]/UIAButton[1]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIATableView[1]/UIAButton[2]</item>
            <item>//UIAApplication[1]/UIAWindow[1]/UIATableView[1]/UIAButton[3]</item>
            <item>
                //UIAApplication[1]/UIAWindow[1]/UIATableView[1]/UIATableCell[2]/UIACollectionView[1]/UIACollectionCell[5]
            </item>
        </blackList>
        <!--触发器 条件>>动作 支持返回,点击,手势密码解锁,延时等待-->
        <trigger>
            <item>分享到>>back</item>
            <item>车辆品牌>>back</item>
            <item>添加房产|开始评估>>back</item>
            <item>社保计算|账户自动同步>>back</item>
            <item>我的权益>>delay->8</item>
            <item>进入平安行>>back</item>
            <item>选择银行>>back</item>
            <item>选择保险公司>>back</item>
            <item>检测到新版本>>back</item>
            <item>取消|确定|添加车辆>>back</item>
            <item>温馨提示|确定>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>温馨提示|立即开通|取消>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>温馨提示|贷款期数不能为空>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>温馨提示|确定|取消>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>提示|确定|取消>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>安全验证|立即验证|以后再说>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>closeButton>>//UIAApplication[1]/UIAWindow[1]/UIAButton[1]</item>
            <item>安全验证|立即验证|以后再说>>//UIAApplication[1]/UIAWindow[4]/UIAButton[1]</item>
            <item>更多解锁方式>>gesture->//UIAButton[@name='blue circle']</item>
        </trigger>
        <!--1:clazz+name+label+value+path 2:clazz+name+label+value+path 3:taskID-->
        <filter>1</filter>
        <!--退出遍历的条件:重复在同一窗口的迭代次数-->
        <allowSameWinTimes>10</allowSameWinTimes>
    </rule>
    <log>
        <ios>idevicesyslog -d -u #udid#|grep PANewToapAPP</ios>
    </log>
    <global>
        <!--Appium port-->
        <port>5757</port>
        <!--Appium host-->
        <host>127.0.0.1</host>
        <!--测试类型 1.android 2.ios 3.web-->
        <mode>2</mode>
        <!--遍历深度-->
        <depth>3</depth>
        <!--截图和视频的目录-->
        <screenshot>/Users/mac/Desktop/png</screenshot>
        <!--遍历时间 分-->
        <duration>0</duration>
        <!--延时等待 秒-->
        <interval>1</interval>
        <!--超时 秒-->
        <timeout>60</timeout>
    </global>
</config>